@page
@model MenuModel





<div class="row">

    <header class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <h3>
            online menu
        </h3>
    </header>
    @foreach (var r in Model.menulist)
    {


        <div id="menu" class="col-lg-3 col-md-3 col-sm-3 col-xs-12" style="">

            <fieldset>

                <div style="height:180px; width:100%;">


                    <img src="@Url.Content(r.ImageReferaceId)" />


                    @*<img src="@Url.Content("data:image/jpeg;base64,")" alt="tbd" />*@
                </div>
                <div>
                    <label for="">Pizza Name:</label>
                </div>
                <div>
                    <label for="" style="">$5.99:</label> <br />
                    <input type="button" value="order now" class="submit" />
                </div>

            </fieldset>

        </div>




    }
    <div>
        <input type="submit" asp-page="/Account" value="Start order" class="submit" style="width:59%" />

        @*<input type="button" class="submit" value="place order" />*@
    </div>

    <hr />
</div>


<style>
    .row {
        margin: 0%;
        padding: 0%;
        height: 650px;
        width: 100%;
        margin-top: 100px;
        border: 1px solid lightgray;
